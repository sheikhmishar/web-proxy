{
  "name": "web-proxy",
  "version": "1.0.0",
  "description": "A simple web proxy in node js",
  "keywords": [
    "proxy",
    "web",
    "http",
    "http proxy",
    "ws",
    "websocket",
    "websocket proxy",
    "node",
    "nodejs"
  ],
  "author": {
    "name": "sheikhmishar",
    "email": "misharex.sezan@gmail.com"
  },
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "watch": "bash -c \"export DEBUG=* && nodemon\"",
    "watch:prod": "set \"NODE_ENV=production\" && nodemon",
    "watch:debug": "nodemon -e js,json -x \"node --inspect-brk server.js\""
  },
  "engines": {
    "node": "v12.16.1",
    "npm": "6.13.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6"
  },
  "devDependencies": {
    "mocha": "^8.1.3"
  },
  "optionalDependencies": {
    "@types/express": "^4.17.8",
    "@types/http-proxy-middleware": "^0.19.3"
  }
}
